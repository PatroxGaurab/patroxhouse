var currently_selected = "desc_div1";
$(function()
{
     $("a#ca1").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div1").slideToggle();
	 currently_selected = "desc_div1";
         return false;
     }); 

     $("a#ca2").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div2").slideToggle();
	 currently_selected = "desc_div2";
         return false;
     }); 
     $("a#ca3").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div3").slideToggle();
	 currently_selected = "desc_div3";
         return false;
     }); 
     $("a#ca4").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div4").slideToggle();
	 currently_selected = "desc_div4";
         return false;
     }); 
     $("a#event1").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div5").slideToggle();
	 currently_selected = "desc_div5";
         return false;
     }); 
     $("a#event2").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div6").slideToggle();
	 currently_selected = "desc_div6";
         return false;
     }); 
     $("a#event3").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div7").slideToggle();
	 currently_selected = "desc_div7";
         return false;
     }); 

     $("a#ldiv1").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div5").slideToggle();
	 currently_selected = "desc_div5";
         return false;
     }); 
     $("a#ldiv2").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div6").slideToggle();
	 currently_selected = "desc_div6";
         return false;
     }); 
     $("a#ldiv3").click(function()
     {
         $("#"+currently_selected).slideToggle();
         $("#desc_div7").slideToggle();
	 currently_selected = "desc_div7";
         return false;
     }); 
var hide_me_flag = true;
     $("a#hide_me_controller").click(function()
     {
         $("#hide_me").slideToggle();
	 if(hide_me_flag){
		hide_me_flag = false;	
		$("#hide_me_controller").html("Less");
	 }
	 else{
		hide_me_flag = true;
		$("#hide_me_controller").html("Learn More");
	 }
         return false;
     }); 
});
	</script>
